{
  "name": "supabase-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for querying Supabase fact tables with analytical capabilities",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/sse-server.js",
    "start:stdio": "node dist/index.js",
    "start:sse": "node dist/sse-server.js",
    "dev": "tsc --watch & node --watch dist/index.js",
    "dev:http": "tsc --watch & node --watch dist/http-server.js",
    "dev:sse": "tsc --watch & node --watch dist/sse-server.js"
  },
  "keywords": [
    "mcp",
    "supabase",
    "analytics"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3"
  }
}

